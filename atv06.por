programa {
  funcao inicio() {

    real a, b, c, vt, vm

    escreva("Digite o valor da hora: ")
    leia(a)

    escreva("Digite a Horas trabalhadas no dia: ")
    leia(b)

    escreva("Digite os Dias trabalhados: ")
    leia(c)

    vt = a * b
    vm = vt * c

    escreva("O salário do Mês é R$: ", vm)


    
  }
}
